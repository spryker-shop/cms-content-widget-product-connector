{
  "name": "spryker-shop/cms-content-widget-product-connector",
  "type": "library",
  "description": "CmsContentWidgetProductConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/cms-content-widget": "^1.2.0",
    "spryker/kernel": "^3.30.0",
    "spryker/product-storage": "^1.8.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*",
    "spryker/twig": "*"
  },
  "suggest": {
    "spryker/twig": "If you want to use CmsProductContentWidgetPlugin."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "cs-check": "phpcs --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -p -s src/",
    "cs-fix": "phpcbf --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -p -s src/"
  }
}
